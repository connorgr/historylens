
# Our JSON format:
# [ { "document_url" : string,
#     "origin" : number?,
#     "authors" : [(string?, string?)]    // (firstName, lastName)
#     "topics" : [string]
#     "year" : number?
#     "places_referenced" : { number : number }   // (place, count)
#     "title" : string?
#   } ]

# items format:
# {"items":[{"id":NUM/STR,
#            "title":title,
#            "author"[{"family":first, "given":last}],
#            "issued":STR}]}
# 


